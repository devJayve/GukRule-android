<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute,ResourceCycle"
    >

    <!--light-->
    <font
        android:font="@font/nanum_square_acl"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/nanum_square_acl"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <!--regular-->
    <font
        android:font="@font/nanum_square_acr"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/nanum_square_acr"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--bold-->
    <font
        android:font="@font/nanum_square_acr"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/nanum_square_acr"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!--extra-bold-->
    <font
        android:font="@font/nanum_square_aceb"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/nanum_square_aceb"
        app:fontStyle="normal"
        app:fontWeight="1000" />

</font-family>